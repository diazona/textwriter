<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/1">
            <name>textwriter</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <web-module-libraries>
                <library>
                    <file>${libs.servlet24.classpath}</file>
                </library>
                <library>
                    <file>${libs.jsp20.classpath}</file>
                </library>
                <library>
                    <file>${libs.jstl11.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${reference.ellipsix.jar}</file>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>ellipsix</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
